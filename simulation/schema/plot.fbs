namespace Visualisation;

table Plot {
    key:string (key, required);
    title:string;
    xlabel:string;
    ylabel:string;
    x:[float] (required);
    y:[float] (required);
}

table PlotGroup {
    title:string (key, required);
    plots:[Plot];
}

table PlotBundle {
    plot_groups: [PlotGroup];
}

root_type PlotBundle;
